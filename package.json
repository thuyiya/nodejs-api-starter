{
  "name": "nodejs-api-starter",
  "version": "0.1.0",
  "description": "Starter kit for nodejs api",
  "main": "app.js",
  "scripts": {
    "start:prod": "babel-node --presets es2015 app.js",
    "start": "nodemon --exec babel-node --presets es2015 app.js",
    "debug": "babel-node --presets es2015 --debug-brk app.js",
    "test": "mocha test --compilers js:babel-register",
    "pm2": "pm2 start app.js --name='dev' --interpreter ./node_modules/.bin/babel-node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thuyiya/nodejs-api-starter.git"
  },
  "author": "thusitha jayalath",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thuyiya/nodejs-api-starter/issues"
  },
  "keywords": [
    "es6",
    "babel",
    "node",
    "eslint",
    "express",
    "nodemon",
    "redis"
  ],
  "homepage": "https://github.com/thuyiya/nodejs-api-starter#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "mongoose": "^5.0.3",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "dotenv": "^5.0.0",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^5.0.0",
    "should": "^13.2.1",
    "util": "^0.10.3"
  },
  "apidoc": {
    "name": "Nodejs api Starter Kit",
    "version": "0.1.0",
    "description": "Starter Kit api documentation",
    "apidoc": {
      "title": "Starter Kit - apidoc",
      "url": "https://api.github.com/v1"
    }
  }
}
